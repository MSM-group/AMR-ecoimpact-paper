p_adj_stars = function(p){
case_when(p > 0.05 ~ "ns",
p <= 0.05 & p > 0.01 ~ "*",
p <= 0.01 & p > 0.001 ~ "**",
p <= 0.001 & p > 0.0001 ~ "***",
p <= 0.0001 ~ "****")
}
